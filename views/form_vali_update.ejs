                    <br>
                    <span class="error" style="color: red; font-size: small;"></span>

                    <br><br>

                    <label for="city">City</label>
                    <input type="text" name="city" id="city" value ='<%= item.Basic_Detail_v[0].city %>'>
                    <br>
                    <span class="error" style="color: red; font-size: small;"></span>

                    <br><br>

                    <label for="gender">Gender</label>
                    <input type="radio" id="male" name="gender" class="gender" value="male"
                    <% if( item.Basic_Detail_v[0].gender == "male") { %> checked 
                        <% } %> 
                    >
                    <label for="male" for="male">Male</label>
                    <input type="radio" id="female" name="gender" class="gender" value="female"
                    <% if( item.Basic_Detail_v[0].gender == "female") { %> checked 
                        <% } %> 
                    >
                    <label for="female">Female</label>
                    <br>
                    <span class="errors error" style="color: red; font-size: small;"></span>

                    <br><br>

                    <label for="state">State</label>
                    <select name="state" id="state">
                        <option value="">Select State</option>
