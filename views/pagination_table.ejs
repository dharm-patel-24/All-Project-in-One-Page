<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    
</head>
<body>
    <%- include ('pagination_component') %>
    <br>
    <table border="1" style="color: aliceblue; background-color: rgb(40, 108, 110); justify-content: center; text-align: center;">
        <tr>
            <th>Student Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Id</th>
            <th>Phone Number</th>
            <th>Stream</th>
            <th>Age</th>
            <th>City</th>
            <th>Gender</th>
            <th>Created Date</th>
        </tr>
        <% data.forEach(function(item) { %>
            <tr>
                <td>
                    <%= item.std_id %>
                </td>
                <td>
                    <%= item.first_name %>
                </td>
                <td>
                    <%= item.last_name %>
                </td>
                <td>
                    <%= item.email %>
                </td>
                <td>
                    <%= item.number %>
                </td>
                <td>
                    <%= item.stream %>
                </td>
                <td>
                    <%= item.age %>
                </td>
                <td>
                    <%= item.city %>
                </td>
                <td>
                    <%= item.gender %>
                </td>
                <td>
                    <%= item.created_date %>
                </td>
            </tr>
        <% }); %>
    </table>
</body>
</html>

