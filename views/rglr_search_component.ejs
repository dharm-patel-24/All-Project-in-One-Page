    <button class=" currentPage"><%= currentPage %></button>

    <a href='/rglr-search-data?page=<%=Number(currentPage)+1%>&querys=<%= querys %>'><button <% if(currentPage==totalPages){ %> disabled || onclick="return false;" <% } %>> > </button></a>
    <a href='/rglr-search-data?page=<%=Number(totalPages)%>&querys=<%= querys %>'><button <% if(currentPage==totalPages){ %> disabled <% } %>> >> </button></a>
</div>