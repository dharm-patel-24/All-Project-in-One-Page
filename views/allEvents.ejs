<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JS</title>
</head>
<style>
    .clr {
        color:grey;
    }
</style>
<body onload="Onloaded()" onresize="resize()" onhashchange="hashchange()">
    <table border="1"cellspacing="0" cellpadding="50">
        <tr>
            <td class="clr"><a onclick="onclicked()">Onclick</a></td>
            <td class="clr"><input onblur="onblured()" placeholder="Onblure" size="7"></td>
            <td class="clr"><a ondblclick="ondouble()">Ondblclick</a></td>
            <td class="clr"><p ondrag="dragging(event)" draggable="true">Drag me!</p></td>
        </tr>

        <tr>
            <td class="clr"><input type="text" onkeypress="onkeypressed()" placeholder="Press any Key"></td>
            <td class="clr"><input type="text" onkeydown="keydown()" placeholder="Key Down"></td>
            <td class="clr"><img id="img" onmouseover="bigImg()" onmouseout="normalImg()" border="0" src="data:image/png;base64," alt="Smiley" width="32" height="32"></td>
            <td class="clr"><input id="foc" type="text" onfocus="onfocused()" placeholder="Click Me!"></td>
        </tr>

        <tr>
            <td class="clr"><img src="image.gif" onerror="onerr()"></td>
            <td class="clr">
                <select id="mySelect" onchange="change()">
                    <option value="Audi">Audi</option>
                    <option value="BMW">BMW</option>
                    <option value="Mercedes">Mercedes</option>
                    <option value="Volvo">Volvo</option>
              </select>
            </td>
            <td class="clr"><input type="text" oninput="onInpu()" placeholder="Oninput"></td>
            <td class="clr"><input type="text" onmouseleave="onmleave()" placeholder="Onmouseleave"></td>
        </tr>

        <tr>
            <td class="clr">
                <form onreset="reset()">
                <input type="reset">
                </form>
            </td>
            <td class="clr"><input type="text" value="Select me!!" onselect="select()"></td>
            <td class="clr"><input type="text" value="Copy Me" oncopy="copyme()"></td>
            <td class="clr"><input type="text" value="Cut Me" oncut="cut()"></td>
        </tr>

        <tr>
            <td class="clr"><input type="text" value="Paste here" onpaste="paste()"></td>
            <td class="clr"><input type="text" id="keyup" onkeyup="keyup()" placeholder="Onkeyup"></td>
            <td class="clr"><video src="https://www.w3schools.com/jsref/mov_bbb.mp4" controls onplay="pplay()"></video>Onplay</td>
            <td class="clr"><video src="https://www.w3schools.com/jsref/mov_bbb.mp4" controls onpause="pausee()"></video>Onpause</td>
        </tr>

        <tr>
            <td class="clr"><video src="https://www.w3schools.com/jsref/movie.mp4" controls onprogress="progressed()"></video>Onprogress</td>
            <td class="clr"><video src="" onended="endede()"></video></td>
            <td class="clr"><video src="" onvolumechange="volchange()"></video></td>
            <td class="clr"></td>
        </tr>

    </table>

    <script>
        function onclicked() {
            alert("Onclick event is triggred!");
        }

        function onblured() {
            alert("Onblure event is triggred!");
        }

        function ondouble() {
            alert("Ondoubleclick event is triggred!");
        }

        function dragging(event) {
            alert("Dragging event is triggred!");
        }

        function onkeypressed() {
            alert("Onkeypress is triggred!");
        }

        function keydown() {
            alert("Onkeydown is triggred!");
        }

        function bigImg() {
            let x = document.getElementById("img");
            x.style.width = "64px";
            x.style.height = "64px";
        }

        function normalImg() {
            let x = document.getElementById("img");
            x.style.width = "32px";
            x.style.height = "32px";
        }

        function onfocused() {
            let x = document.getElementById("foc");
            x.style.background = "yellow";
        }

        function onerr() {
            alert('The image could not be loaded.');
        }

        function change() {
            alert("Onchange is triggred!");
        }

        function Onloaded() {
            alert("Onload is triggerd!");
        }

        function onInpu() {
            alert("Oninput is triggred!");
        }

        function onmleave() {
            alert("On mouse leave is triggred!");
        }

        function resize() {
            alert("Resize is triggred!");
        }

        function hashchange() {
            alert("Onhashchange is triggred!")
        }

        function reset() {
            alert("Onreset is triggred!");
        }

        function select() {
            alert("Onselect is triggred!");
        }

        function copyme() {
            alert("Oncopy is triggred!");
        }

        function cut() {
            alert("Oncut is triggred!");
        }

        function paste() {
            alert("Onpaste is triggerd!");
        }

        function keyup() {
            alert("Onkeyup is triggred!");
        }

        function pplay() {
            alert("Onplay is triggred!");
        }

        function pausee() {
            alert("Onpause is triggred!");
        }

        function progressed() {
            alert("Onprogress is triggred!");
        }
    </script>
</body>
</html>